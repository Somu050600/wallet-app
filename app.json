{
  "expo": {
    "newArchEnabled": true,
    "name": "wallet-app",
    "slug": "wallet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.somu0506.walletapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.somu0506.walletapp"
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    },
    "plugins": [
      "./plugins/withKotlinJvmTarget",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-video",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID to share cards securely."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "eeff084a-8867-4917-94e9-175e4bf1b319"
      }
    }
  }
}
